<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <Authors>Oliver Czibula</Authors>
    <Product>Seating Plan Optimization</Product>
    <Description>A decision support system for solving the problem of assigning guests to tables for events such as weddings</Description>
    <RepositoryUrl>https://github.com/Ozzah/SeatingPlanOptimization</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <OutputPath>..\..\bin\</OutputPath>
    <AppendTargetFrameworkToOutputPath>False</AppendTargetFrameworkToOutputPath>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>Ozzah.SeatingPlanOptimization</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.8.0" />
    <PackageReference Include="Google.OrTools" Version="9.2.9972" />
  </ItemGroup>

</Project>
